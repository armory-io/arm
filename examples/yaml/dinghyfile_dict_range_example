application: dictrangeexample
pipelines:
- name: Loop Example
  application: dictrangeexample
  stages:
{{ $dictdev := dict "waitTime" "10" "name" "dev" }}
{{ $dictqa := dict "waitTime" "20" "name" "qa" }}
{{ $myenvs := list $dictdev $dictqa }}
{{ range $myenvs }}
  - waitTime: {{ get . "waitTime" }}
    name: {{ get . "name" }}
    type: wait
{{ end }}
