{
  "application": "listrangeexample",
  "pipelines": [
    {
      "name": "Loop Example",
      "application": "listrangeexample",
      "stages": [
        {{ $listdev := list "10" "dev" }}
        {{ $listqa := list "20" "qa" }}
        {{ $myenvs := list $listdev $listqa }}
        {{ $count := 1 }}
        {{ range $myenvs }}
            {
                "type": "wait",
                "waitTime" :  {{ index . 0  | toJson }},
                "name" :  {{ index . 1  | toJson }}
            }
            {{ if ne $count (len $myenvs) }}
                {{ $count = add $count 1 }}
                ,
            {{ end }}
        {{ end }}
      ]
    }
  ]
}
